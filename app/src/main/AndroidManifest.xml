<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.schoolchat.schoolchat">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".UserInterface.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- actividad del LogIn -->
        <activity
            android:name=".UserInterface.LogInActivity"
            android:label="@string/title_LogIn">

        </activity>
        <!--actividad del registro-->
        <activity
            android:name=".UserInterface.Altausuario"
            android:label="@string/title_Altausuario">

        </activity>
    </application>

</manifest>